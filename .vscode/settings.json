{
  "typescript.tsdk": "node_modules/typescript/lib",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.rulers": [80, 120]
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.rulers": [80, 120]
  },
  "files.exclude": {
    "**/node_modules": true
  },
  "nixEnvSelector.nixFile": "${workspaceRoot}/shell.nix",
  "search.exclude": {
    "**/node_modules": true,
    "package-lock.json": true
  },
  "vite.port": 3000
}
